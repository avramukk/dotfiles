[
  {
    "context": "Workspace",
    "bindings": {
      "ctrl-`": "workspace::ToggleBottomDock",
      "shift-z": "workspace::ToggleZoom"
    }
  },
  {
    "context": "Editor",
    "bindings": {
      "alt-escape": "editor::ShowCompletions",
      "alt-enter": "editor::ToggleCodeActions",
      "cmd-b": "editor::GoToDefinition",
      "cmd-shift-b": "editor::GoToTypeDefinition"
    }
  }
]
